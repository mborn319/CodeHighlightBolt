{
    "name": "royallthefourth/codehighlightbolt",
    "description": "Automatic code highlighting for your Bolt site with highlight.js",
    "type": "bolt-extension",
    "keywords": [
        "code",
        "highlighting",
        "css"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Royall Spence",
            "email": "royall@royall.us",
            "homepage": "http://royall.us"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\royallthefourth\\CodeHighlightBolt\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\royallthefourth\\CodeHighlightBolt\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\royallthefourth\\CodeHighlightBolt\\CodeHighlightBoltExtension",
        "bolt-icon": "icon-codehighlight.png"
    }
}
